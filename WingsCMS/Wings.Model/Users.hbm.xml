<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2">
  <class name = "Wings.Model.Users, Wings.Model" table="users" lazy="false">
    <id name="LogonId" column="LogonId" type="String(20)">
      <generator class = "assigned" />
    </id>
      <property name = "Name" column="Name" type="String(40)"/>
      <property name = "Password" type="String(20)"/>
      <property name = "EmailAddress" type="String(40)"/>
      <property name = "LastLogon" type="DateTime"/>
  </class>
</hibernate-mapping>