    <script type="text/javascript">
        DWZInit();
        function DWZInit() {
            DWZ.init("/content/admin/config/dwz.frag.xml", {
                loginUrl: "login_dialog.html", loginTitle: "登录", // 弹出登录对话框
                //		loginUrl:"login.html",	// 跳到登录页面
                statusCode: { ok: 200, error: 300, timeout: 301 }, //【可选】
                pageInfo: { pageNum: "pageNum", numPerPage: "numPerPage", orderField: "orderField", orderDirection: "orderDirection" }, //【可选】
                debug: false, // 调试模式 【true|false】
                callback: function () {
                    initEnv();
                    $("#themeList").theme({ themeBase: "/content/admin/themes" }); // themeBase 相对于index页面的主题base路径
                }
            });
        }
        function openDialogNo(url, dlgId, title, width, height) {
            $.pdialog.openNo(url, dlgId, title, { width: width, height: height });
        }
        function openDialog(url, dlgId, title, width, height) {
            $.pdialog.open(url, dlgId, title, { width: width, height: height });
        }
        function openTab(tabid, url, title) {
            navTab.openTab(tabid, url, { title: title, data: {}, fresh: false, external: true });
        }
        function DelClick(url) {
            var $this = $(this);
            var title = "确定要删除吗";
            if (title) {
                alertMsg.confirm(title, {
                    okCall: function () {
                        ajaxTodo(url);
                    }
                });
            } else {
                ajaxTodo(url);
            }
        }
    </script>
