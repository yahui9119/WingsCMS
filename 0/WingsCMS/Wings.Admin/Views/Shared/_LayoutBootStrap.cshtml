<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/b.main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/icons.css")" rel="stylesheet" type="text/css" />
    @RenderSection("CSS", required: false)
</head>
<body>
    @RenderBody()
    <script src="@Url.Content("~/Content/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/javascript/b.main.js")" type="text/javascript"></script>
    @RenderSection("JS", required: false)
</body>
</html>