@Mui.Language:<a id='clang' href="">@ViewBag.lang</a>
<script type='text/javascript'>
    $(function () {
        var isopen = false;
        $('#clang').click(function (e) {
            e.preventDefault();
            if (isopen) {
                $('#clang').next().remove();
                isopen = false;
                return false;
            }
            $.post('@Url.Action("langs","mui")', function (r) {
                $('#clang').after(r);
                isopen = true;
            });
        });
    });
</script>

